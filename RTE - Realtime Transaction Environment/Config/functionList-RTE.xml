<NotepadPlus>
	<functionList>
		<associationMap>
			<association id=         "rte_function"      userDefinedLangName="RTE"               />
			<association id=         "rte_function"      ext=".rte"                              />
		</associationMap>
		<parsers>
			<!-- ========================================================= [ RTE ] -->

			<!--
			|    https://notepad-plus-plus.org/community/topic/18108/regex-in-functionlist-xml
			\-->
			<parser 
				displayName="RTE - Realtime Transaction Environment for TradExpress Â©Generix"
				id         ="rte_function" 
			>
				<function
					displayName="RTE"
					mainExpr   ="(?x)
							(^(begin)$)
						"
				>
					<functionName>
						<funcExpr expr="(^(begin)$)" />
					</functionName>
				</function>
			</parser>

			<!-- ================================================================= -->
		</parsers>
	</functionList>
</NotepadPlus>
