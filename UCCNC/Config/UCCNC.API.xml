    <KeyWord name="exec.IsLoading" func="yes">
      <Overload retVal="{bool}" descr="returns&gt; true if software is loading a g-code file">
        <Param name="void" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.IsMoving" func="yes">
      <Overload retVal="{bool}" descr="returns&gt; true if software is executing motion or function">
        <Param name="void" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Ismacrostopped" func="yes">
      <Overload retVal="{bool}" descr="checks if a stop was pressed by the user on the UCCNC software GUI">
        <Param name="void" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Loadfile" func="yes">
      <Overload retVal="{void}" descr="loads a g-code file. The parameter is a string which is the full path of the file to be loaded.">
        <Param name="string &quot;path/filename&quot;" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Pluginshowup" func="yes">
      <Overload retVal="{void}" descr="calls a Plugin file's Showup_event(); function">
        <Param name="string Pluginfilename" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Question" func="yes">
      <Overload retVal="{double}" descr="shows a Question form &amp; waits for a DOUBLE value as the answer">
        <Param name="string &quot;Questiontext&quot;" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Readkey" func="yes">
      <Overload retVal="{string}" descr="reads a key value from the profile (.pro) file">
        <Param name="string section" />
        <Param name="string key" />
        <Param name="string defaultvalue" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Resetswapaxis" func="yes">
      <Overload retVal="{void}" descr="resets swap axis sequences">
        <Param name="void" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.SetModbusregister" func="yes">
      <Overload retVal="{bool}" descr="Writes a single register in the modbus register table">
        <Param name="int Registernumber" />
        <Param name="ushort Value" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.SetModbusregisters" func="yes">
      <Overload retVal="{bool}" descr="Writes multiply registers in the modbus register table">
        <Param name="int Startregister" />
        <Param name="ushort[] Values" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Setcurrenttool" func="yes">
      <Overload retVal="{void}" descr="sets the selected tool number in the UCCNC software">
        <Param name="int toolnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Setofflinemode" func="yes">
      <Overload retVal="{void}" descr="sets machine to offline mode or online mode">
        <Param name="bool true/false" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Setoutpin" func="yes">
      <Overload retVal="{void}" descr="Sets the selected output pin to high level">
        <Param name="int portnumber" />
        <Param name="int pinnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Showplugin" func="yes">
      <Overload retVal="{int (0-3)}" descr="calls the Showup event of a UCCNC plugin installed in the /Plugins directory">
        <Param name="string pluginfilename" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Slaveaxis" func="yes">
      <Overload retVal="{void}" descr="slaves an axis to an axis (to clear, set slave to 0 eg (4,0))">
        <Param name="int masteraxis" />
        <Param name="int slaveaxis" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Stop" func="yes">
      <Overload retVal="{void}" descr="causes an instant stop on all axis">
        <Param name="void" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.StopWithDeccel" func="yes">
      <Overload retVal="{void}" descr="stops all axis using set decceleration profile">
        <Param name="void" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Swapaxis" func="yes">
      <Overload retVal="{void}" descr="swaps step &amp; direction pin numbers &amp; pin negate settings of one axis with another axis">
        <Param name="int axis1" />
        <Param name="int axis2" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.TextQuestion" func="yes">
      <Overload retVal="{string}" descr="shows a Question form &amp; waits for a STRING as the answer">
        <Param name="string &quot;Questiontext&quot;" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Wait" func="yes">
      <Overload retVal="{void}" descr="causes loop to stop for set amount of time in milliseconds">
        <Param name="int milliseconds" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.WriteModbusString" func="yes">
      <Overload retVal="{void}" descr="Converts a string into an ushort array &amp; writes it into the modbus register table">
        <Param name="string String"></Param>
        <Param name="int Startregister" />
        <Param name="bool HightoLowByteorder" />
      </Overload>
    </KeyWord>
    <KeyWord name="exec.Writekey" func="yes">
      <Overload retVal="{void}" descr="writes a key value into the profile (.pro) file">
        <Param name="string section" />
        <Param name="string key" />
        <Param name="string value" />
      </Overload>
    </KeyWord>
