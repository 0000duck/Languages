<NotepadPlus>
  <AutoComplete language="UCCNC">
    <Environment ignoreCase="no" startFunc="( " stopFunc=" )" paramSeparator="," terminal=";" additionalWordChar="."></Environment>
    <KeyWord name="AS3.Additemtolist" func="yes">
      <Overload retVal="{void}" descr="Adds a string value to a Listbox with the corresponding labelnumber">
        <Param name="string val" />
		<Param name="int labelnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.GetLED" func="yes">
      <Overload retVal="{bool}" descr="returns&gt;logic state of a LED screen object">
        <Param name="int LEDnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.Getbutton" func="yes">
      <Overload retVal="{bool}" descr="returns&gt;true if button is current being pressed/input triggered">
        <Param name="int BUTTONnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.Getbuttonstate" func="yes">
      <Overload retVal="{bool}" descr="returns&gt;true if button is in toggled ON state">
        <Param name="int BUTTONnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.Getfield" func="yes">
      <Overload retVal="{string}" descr="Gets/reads the value of a field object (eg DRO)">
        <Param name="int FIELDnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.SetLED" func="yes">
      <Overload retVal="{void}" descr="Sets the logic state of a LED screen object">
        <Param name="bool state" />
        <Param name="int LEDnumber"></Param>
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.Setfield" func="yes">
      <Overload retVal="{void}" descr="Sets value of field (eg numeric DRO).  Call ValidateField nxt">
        <Param name="double value" />
        <Param name="int FIELDnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.Setfieldtext" func="yes">
      <Overload retVal="{void}" descr="Sets value of field (eg text DRO).  Call ValidateField nxt">
        <Param name="string value" />
        <Param name="int FIELDnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.Switchbutton" func="yes">
      <Overload retVal="{void}" descr="Sets toggle type buttons and switches the button to on/off states">
        <Param name="bool Ison" />
        <Param name="int BUTTONnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="AS3.Validatefield" func="yes">
      <Overload retVal="{void}" descr="Validates the value of a field object (Setfield &amp; Setfieldtext)">
        <Param name="int FIELDnumber" />
      </Overload>
    </KeyWord>
    <KeyWord name="Button" />
    <KeyWord name="Controls.Add" />
    <KeyWord name="Convert.ToDouble" func="yes">
      <Overload retVal="{double}" descr="convert to a double">
        <Param name="type" />
      </Overload>
    </KeyWord>
    <KeyWord name="Convert.ToString" func="yes">
      <Overload retVal="{string}" descr="convert to a string">
        <Param name="type" />
      </Overload>
