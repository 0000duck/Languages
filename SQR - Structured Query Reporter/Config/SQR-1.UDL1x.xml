<NotepadPlus>
    <UserLang name="SQR" ext="sqr sqc">
        <Settings>
            <Global caseIgnored="yes"/>
            <TreatAsSymbol comment="no" commentLine="yes"/>
            <Prefix words1="no" words2="yes" words3="no" words4="no"/>
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">'"0'"0</Keywords>
            <Keywords name="Folder+">begin-heading begin-procedure begin-program begin-report begin-select begin-sql begin-setup #ifdef #ifndef evaluate if while</Keywords>
            <Keywords name="Folder-">end-heading end-procedure end-program end-report end-select end-sql end-setup #endif end-evaluate end-if end-while</Keywords>
            <Keywords name="Operators">( ) , / : | + &lt; = &gt;</Keywords>
            <Keywords name="Comment">1 2 0!</Keywords>
            <Keywords name="Words1">#break #current-line #define #include #rows</Keywords>
            <Keywords name="Words2">$ { # &amp; }</Keywords>
            <Keywords name="Words3">add and as break by char clear-array close create-array display do edit else field fill find for-reading for-writing from get input insert instr into isblank isnull let lpad ltrim move name new-page noline not number on-error open or order print put quiet range read record rpad rtrim set show size status stop string substr to to_char to_number translate unstring update upper values vary when when-other wrap write where</Keywords>
            <Keywords name="Words4"/>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="FF8040" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="FF8000" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="800040" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="COMMENT" styleID="1" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="008000" bgColor="FFFF80" fontName="" fontStyle="0"/>
            <WordsStyle name="NUMBER" styleID="4" fgColor="0080FF" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="OPERATOR" styleID="10" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="008000" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="008000" bgColor="FFFFFF" fontName="" fontStyle="0"/>
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0"/>
        </Styles>
    </UserLang>
</NotepadPlus>
